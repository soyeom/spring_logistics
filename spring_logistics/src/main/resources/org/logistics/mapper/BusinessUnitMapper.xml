<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.logistics.mapper.BusinessUnitMapper">
    
    <select id="selectBusinessUnits" resultType="org.logistics.domain.BusinessUnitDTO">
        SELECT
            bu_id AS id,   
            bu_name AS name
        FROM business_unit
        ORDER BY bu_id
    </select>
    
</mapper>